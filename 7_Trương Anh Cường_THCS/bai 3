
tu_so = int(input("Nhập tử số: "))
mau_so = int(input("Nhập mẫu số: "))

ucln = 1
if tu_so < mau_so :
    nho = tu_so
else :
    nho = mau_so
for i in range(1,nho +1) :
        if tu_so % i == 0 and mau_so % i == 0 :
            ucln = i
tu_so_rut_gon = tu_so // ucln 
mau_so_rut_gon = mau_so // ucln
print(tu_so_rut_gon,"/",mau_so_rut_gon)

